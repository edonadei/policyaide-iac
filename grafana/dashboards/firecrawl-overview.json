        {
          "expr": "rate(process_cpu_seconds_total{job=\"firecrawl-api\"}[5m]) * 100",
          "legendFormat": "API CPU",
          "refId": "A"
        },
        {
          "expr": "rate(node_cpu_seconds_total{mode=\"user\"}[5m]) * 100",
          "legendFormat": "System CPU",
          "refId": "B"
        }
      ],
      "title": "CPU Usage",
      "type": "timeseries"
